<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">



    <Button
            android:text="隐式意图打开浏览器"
            android:onClick="startBrowserInvisible"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/startBrowserInvisible"/>
    <Button
            android:text="拨打电话传参"
            android:onClick="startCall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/button6"/>
    <Button
            android:text="模拟发短信"
            android:onClick="startSendMsg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/button7"/>
    <Button
            android:text="测试activity启动模式"
            android:onClick="startRunMode"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/button8"/>

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/parcelableTv"
            android:textSize="20sp"/>

</LinearLayout>